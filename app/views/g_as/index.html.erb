<h1>Listing gas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gas.each do |ga| %>
      <tr>
        <td><%= ga.nome %></td>
        <td><%= link_to 'Show', ga %></td>
        <td><%= link_to 'Edit', edit_ga_path(ga) %></td>
        <td><%= link_to 'Destroy', ga, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ga', new_ga_path %>
